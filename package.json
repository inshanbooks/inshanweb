{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "start": "cross-env SOCKET=true bun ./scripts/next-remote-watch.js ./data",
    "dev": "next dev",
    "build": "next build && bun ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "mdx-bundler": "^8.0.0",
    "next": "^15.0.0",
    "next-themes": "^0.4.0",
    "postcss": "^8.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.1.1",
    "rehype-katex": "^6.0.2",
    "rehype-prism-plus": "^1.1.3",
    "rehype-slug": "^5.0.0",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.33.0",
    "tailwindcss": "^3.4.0",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.0",
    "@svgr/webpack": "^8.0.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globby": "11.0.3",
    "husky": "^9.0.0",
    "inquirer": "^8.1.1",
    "lint-staged": "^15.0.0",
    "next-remote-watch": "^1.0.0",
    "prettier": "2.2.1",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.5.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
